<?xml version='1.0' encoding='utf-8'?>
<widget id="io.corium.cofi" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Cofi</name>
    <description>
        Cofi development preview
    </description>
    <author email="toroidalmirror@gmail.com" href="http://doghunter.ddns.net">
        Corium
    </author>
    <content src="index.html" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <access origin="http://doghunter.ddns.net" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
