<?xml version='1.0' encoding='utf-8'?>
<widget id="io.corium.cofi" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Cofi</name>
    <description>
        Cofi development preview
    </description>
    <author email="toroidalmirror@gmail.com" href="http://doghunter.ddns.net">
        Corium
    </author>
    <content src="index.html" />
    <icon src="icon.png" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <access origin="http://doghunter.ddns.net" />
</widget>


